CREATE TABLE movies (
  Name TEXT NOT NULL,
  Actor TEXT NOT NULL, 
  Actress TEXT NOT NULL, 
  Director TEXT NOT NULL, 
  Year_of_release NOT NULL
);
INSERT INTO movies VALUES ('Maari', 'Dhanush', 'Kajal Aggarwal', 'Balaji Mohan', 2015),('Sarrainodu', 'Allu Arjun ', 'Rakul Preet Singh','Boyapati Srinu',2016),('Baahubali','Prabhas','Tamannaah','S. S. Rajamouli',2015);
SELECT * FROM movies;
SELECT Name FROM movies where Actor="Dhanush";
